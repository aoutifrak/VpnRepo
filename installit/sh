sudo apt update && sudo apt install -y git python3 python3-venv python3-pip docker.io &&git clone https://github.com/aoutifrak/VpnRepo.git
cd VpnRepo
chmod +x install.sh
./install.sh

python3 -m venv venv &&source venv/bin/activate
pip install -r requirements.txt
uvicorn main:app --host 0.0.0.0 --port 8888
